#Mount-point          FStype Device                                             Flags                                       
/preloader            emmc   /dev/block/platform/bootdevice/by-name/preloader   flags=display="preloader";backup=1
/pgpt                 emmc   /dev/block/platform/bootdevice/by-name/pgpt        flags=display="pgpt";backup=1
/boot_para            emmc   /dev/block/platform/bootdevice/by-name/boot_para   flags=display="boot_para";backup=1
/recovery             emmc   /dev/block/platform/bootdevice/by-name/recovery    flags=display="recovery";backup=1;flashimg=1
/misc                 emmc   /dev/block/platform/bootdevice/by-name/para        flags=display="para";backup=1
/expdb                emmc   /dev/block/platform/bootdevice/by-name/expdb       flags=display="expdb";backup=1
/frp                  emmc   /dev/block/platform/bootdevice/by-name/frp         flags=display="frp";backup=1;flashimg=1
/mnt/vendor/nvcfg     ext4   /dev/block/platform/bootdevice/by-name/nvcfg       flags=display="nvcfg";backup=1;wipeingui
/mnt/vendor/nvdata    ext4   /dev/block/platform/bootdevice/by-name/nvdata      flags=display="nvdata";backup=1;wipeingui;wipeonfactoryreset
/metadata             emmc   /dev/block/platform/bootdevice/by-name/metadata    flags=display="metadata";backup=1
/mnt/vendor/protect_f ext4   /dev/block/platform/bootdevice/by-name/protect1    flags=display="protect1";backup=1
/mnt/vendor/protect_s ext4   /dev/block/platform/bootdevice/by-name/protect2    flags=display="protect2";backup=1
/seccfg               emmc   /dev/block/platform/bootdevice/by-name/seccfg      flags=display="seccfg";backup=1
/sec1                 emmc   /dev/block/platform/bootdevice/by-name/sec1        flags=display="sec1";backup=1
/proinfo              emmc   /dev/block/platform/bootdevice/by-name/proinfo     flags=display="proinfo";backup=1
/md1img               emmc   /dev/block/platform/bootdevice/by-name/md1img      flags=display="md1img";backup=1;flashimg=1
/md1dsp               emmc   /dev/block/platform/bootdevice/by-name/md1dsp      flags=display="md1dsp";backup=1;flashimg=1
/md1arm7              emmc   /dev/block/platform/bootdevice/by-name/md1arm7     flags=display="md1arm7";backup=1
/md3img               emmc   /dev/block/platform/bootdevice/by-name/md3img      flags=display="md3img";backup=1
/spmfw                emmc   /dev/block/platform/bootdevice/by-name/spmfw       flags=display="spmfw";backup=1;flashimg=1
/mcupmfw              emmc   /dev/block/platform/bootdevice/by-name/mcupmfw     flags=display="mcupmfw";backup=1;flashimg=1
/nvram                emmc   /dev/block/platform/bootdevice/by-name/nvram       flags=display="nvram";backup=1
/bootloader           emmc   /dev/block/platform/bootdevice/by-name/lk          flags=display="lk";backup=1;flashimg=1
/bootloader2          emmc   /dev/block/platform/bootdevice/by-name/lk2         flags=display="lk2";backup=1;flashimg=1
/loader_ext1          emmc   /dev/block/platform/bootdevice/by-name/loader_ext1 flags=display="loader_ext1";backup=1;flashimg=1
/loader_ext2          emmc   /dev/block/platform/bootdevice/by-name/loader_ext2 flags=display="loader_ext2";backup=1;flashimg=1
/boot                 emmc   /dev/block/platform/bootdevice/by-name/boot        flags=display="boot";backup=1;flashimg=1
/logo                 emmc   /dev/block/platform/bootdevice/by-name/logo        flags=display="logo";backup=1
/dtbo                 emmc   /dev/block/platform/bootdevice/by-name/dtbo        flags=display="dtbo";backup=1;flashimg=1
/tee1                 emmc   /dev/block/platform/bootdevice/by-name/tee1        flags=display="tee1";backup=1;flashimg=1
/tee2                 emmc   /dev/block/platform/bootdevice/by-name/tee2        flags=display="tee2";backup=1;flashimg=1
/lenovoraw            emmc   /dev/block/platform/bootdevice/by-name/lenovoraw   flags=display="lenovoraw";backup=1
/lenovocust           ext4   /dev/block/platform/bootdevice/by-name/lenovocust  flags=display="lenovocust";backup=1;wipeingui
/vendor               ext4   /dev/block/platform/bootdevice/by-name/vendor      flags=display="vendor";backup=1;wipeingui
/system               ext4   /dev/block/platform/bootdevice/by-name/system      flags=display="system";backup=1;wipeingui
/cache                ext4   /dev/block/platform/bootdevice/by-name/cache       flags=display="cache";backup=1;wipeingui
/data                 ext4   /dev/block/platform/bootdevice/by-name/userdata    flags=display="userdata";backup=1;wipeingui;encryptable=/dev/block/platform/bootdevice/by-name/metadata
/flashinfo            emmc   /dev/block/platform/bootdevice/by-name/flashinfo   flags=display="flashinfo";backup=1
/sgpt                 emmc   /dev/block/platform/bootdevice/by-name/sgpt        flags=display="sgpt";backup=1
/external_sd          auto   /dev/block/mmcblk1p1   /dev/block/mmcblk1          flags=display="External-SDcard";storage;wipeingui;removable;backup=0 
/usb_otg              auto   /dev/block/sda1        /dev/block/sda              flags=display="Usb-Otg";storage;removable;backup=0                   

# Flashable ones (since ext4 are not flashable )
/lenovocust_image           emmc   /dev/block/platform/bootdevice/by-name/lenovocust  flags=display="lenovocust";backup=1;flashimg=1;wipeingui
/vendor_image               emmc   /dev/block/platform/bootdevice/by-name/vendor      flags=display="vendor";backup=1;wipeingui;flashimg=1
/system_image               emmc   /dev/block/platform/bootdevice/by-name/system      flags=display="system";backup=1;wipeingui;flashimg=1
/cache_image                emmc   /dev/block/platform/bootdevice/by-name/cache       flags=display="cache";backup=1;wipeingui;flashimg=1
/data_image                 emmc   /dev/block/platform/bootdevice/by-name/userdata    flags=display="userdata";backup=1;wipeingui;flashimg=1
